<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.SmsSequenceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.SmsSequence">
        <id column="id" property="id" />
        <result column="number" property="number" />
        <result column="content" property="content" />
        <result column="sent_date" property="sentDate" />
        <result column="sent_status" property="sentStatus" />
        <result column="sent_result" property="sentResult" />
        <result column="sent_result_message" property="sentResultMessage" />
        <result column="out_request_id" property="outRequestId" />
        <result column="out_response_id" property="outResponseId" />
        <result column="create_date" property="createDate" />
        <result column="status" property="status" />
        <result column="buss_id" property="bussId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, number, content, sent_date, sent_status, sent_result, retry_count, create_date, status
    </sql>

</mapper>

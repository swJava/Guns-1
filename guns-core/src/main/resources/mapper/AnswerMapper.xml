<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.AnswerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.Answer">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="student_code" property="studentCode" />
        <result column="begin_date" property="beginDate" />
        <result column="end_date" property="endDate" />
        <result column="duration" property="duration" />
        <result column="score" property="score" />
        <result column="remark" property="remark" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code, student_code, begin_date, end_date, duration, score, remark, status
    </sql>

</mapper>
